{
  "name": "react-cool-starter",
  "version": "2.2.0",
  "description": "A starter boilerplate for an universal web application with the best development experience and best practices.",
  "main": "index.js",
  "engines": {
    "node": ">=5.0 <8",
    "npm": ">=3.0 <5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wellyshen/react-cool-starter"
  },
  "keywords": [
    "starter",
    "boilerpate",
    "universal",
    "react",
    "redux",
    "react router",
    "express",
    "webpack 2",
    "es6",
    "flow",
    "hot reloading",
    "babel",
    "postcss",
    "jest",
    "unit testing",
    "performance optimizing"
  ],
  "contributors": [
    "WellyShen <hivoid19@gmail.com> (https://github.com/wellyshen)",
    "Ivan Kotov <kotovivan@gmail.com> (https://github.com/ivn-cote)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wellyshen/react-cool-starter/issues"
  },
  "homepage": "https://github.com/wellyshen/react-cool-starter",
  "scripts": {
    "scaffold:component": "yo not-bad-react-component",
    "start": "npm run build && npm run start:production",
    "start:dev": "cross-env NODE_ENV=development nodemon ./index.js --exec babel-node",
    "start:production": "cross-env NODE_ENV=production node ./index.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./tools/webpack/webpack.client.babel.js",
    "build:server": "webpack --progress --hide-modules --config ./tools/webpack/webpack.server.babel.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint ./src ./tools ./index.js",
    "lint:css": "stylelint \"./src/**/*.scss\" --syntax scss",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "cross-env NODE_ENV=test jest --coverage --config ./tools/jest/config.json",
    "test:watch": "npm run test --watch",
    "clean": "npm run clean:client && npm run clean:server && npm run clean:test",
    "clean:client": "rimraf ./build/public/assets",
    "clean:server": "rimraf ./build/server.js ./build/server.js.map",
    "clean:test": "rimraf ./coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls && npm run clean:test"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "config": "^1.26.1",
    "express": "^4.15.2",
    "helmet": "^3.6.0",
    "history": "^4.6.1",
    "hpp": "^0.2.2",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "opn": "^5.0.0",
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "redbox-react": "^1.3.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.2",
    "webpack-isomorphic-tools": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "0.46.0",
    "generator-not-bad-react-component": "^1.1.0",
    "image-webpack-loader": "^3.3.1",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.1",
    "nock": "^9.0.13",
    "node-sass": "^4.5.2",
    "nodemon": "^1.11.0",
    "postcss": "^6.0.1",
    "postcss-loader": "^2.0.5",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-node-externals": "^1.6.0"
  }
}
